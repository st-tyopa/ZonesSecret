function checkStalker(npc_community, gulag_type, npc_rank, se_obj)
	if gulag_type == "dv_svobbase" or
		gulag_type == "dark_svob_blokp" then
		return npc_community == "freedom"
	end

	if gulag_type == "dv_grehbase" then
		return npc_community == "greh"
	end
	
	if gulag_type == "dv_svobbase_nb" then
		return npc_community == "freedom" or npc_community == "dolg"
	end
	
	if gulag_type == "dv_dolg_group" then
		return npc_community == "dolg"
	end
	return false
end

