function load_states(gname, type)
	if type == "dv_svobbase" or
	type == "dv_svobbase_nb" or
	type == "dark_svob_blokp" or
	type == "dv_grehbase" or
	type == "dv_dolg_group"	then 
		return function(gulag)
			local actor = db.actor
			if actor ~= nil then
				return 0
			else	
				return gulag.state
			end		
		end
	end
	return nil
end

