function checkStalker(npc_community, gulag_type, npc_rank, se_obj)
	if gulag_type == "agr_dolgbase" then
		return npc_community == "dolg"
	end
	if gulag_type == "agr_dolgblokpost" then
		return npc_community == "dolg"
	end
	if gulag_type == "agr_mercbase" then
		return npc_community == "killer"
	end
	return false
end

