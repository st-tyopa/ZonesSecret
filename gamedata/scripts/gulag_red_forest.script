	if actor ~= nil then				return 0			else					return gulag.state			end				end	end	if type == "redfor_hunters" then 		return function(gulag)			local actor = db.actor			if actor ~= nil then				return 0			else					return gulag.state			end				end	end	if type == "redfor_military" then 		return function(gulag)			local actor = db.actor			if actor ~= nil then				return 0			else					return gulag.state			end				end	endend---------------------------------------------------------------------------- Dynamic ltx--------------------------------------------------------------------------function load_ltx(gname, type)	return nilendfunction checkStalker(npc_community, gulag_type, npc_rank, se_obj)	if gulag_type == "redfor_military" then		return npc_community == "military"	end	if gulag_type == "redfor_bridge_ld" then		return npc_community == "last_day"	end	if gulag_type == "redfor_hunters" then		return npc_community == "stalker"	end	return falseendfunction checkMonster(npc_community, gulag_type)	return falseend