function checkStalker(npc_community, gulag_type, npc_rank, se_obj)
	if gulag_type == "pz_merclager" then
		return npc_community == "killer"
	end
	
	if gulag_type == "pz_fraction_war_stalkers" then
		return npc_community == "stalker"
	end
	
	if gulag_type == "pz_fraction_war_bandits" then
		return npc_community == "bandit"
	end
	
	if gulag_type == "pz_fraction_war_zombiedstalkers" then
		return npc_community == "zombied"
	end
	return false
end

