function load_states(gname, type)
	if type == "pz_merclager" or
	type == "pz_fraction_war_stalkers"	or 	
	type == "pz_fraction_war_bandits"	or 
	type == "pz_fraction_war_zombiedstalkers"	then
		return function(gulag)
			local actor = db.actor
			if actor ~= nil then
				return 0
			else	
				return gulag.state
			end		
		end
	end
	return nil
end

